#include<bits/stdc++.h>
using namespace std;
#define N 10000

bool prime[N];
void seive(int n)
{
    for(int i=2; i<n; i++)
    {
        if(prime[i]==false)
        {
            for(int j=i+i; j<=n; j+=i)prime[j]=true;
        }
    }

}

bool isPrime(int n)
{
    if(prime[n]==false)return true;
    else false;
}

int main()
{
    int n;
    cin>>n;
    seive(n);
    for(int i=2; i<=n; i++)
    {
        if(prime[i]==false)cout<<i<<" ";
    }

    int m;
    cout<<"\nPrime or Not? : ";
    cin>>m;

    if(isPrime(m))cout<<"Prime"<<endl;
    else cout<<"Not Prime"<<endl;

    return 0;
}

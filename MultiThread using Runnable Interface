
public class MyClass {
    public static void main(String args[])throws InterruptedException {
        
    myThread obj = new myThread();// main thread obj create korlo
    //obj.start(); amra eta korte parbona karon eta Runnable interface k implement korse... Runnable er mddhe start func nai
    
    Thread x = new Thread(obj); // so eta solution ekta thread class er object create korbo
    x.start();// ekhn etar sthe ei run() methode k linkup korte hbe tai amr myThread er obj ke parameter hisebe pass kore debo
     
 
     
    }

}

// evhbei ek thread theke arek thread e giye kaj krbe
//start eta dye thread k active kore fele then run() methode execute kore

class myThread extends A implements Runnable
{
    public void run()
    {
        for(int i=0;i<10;i++)
        {
            
         System.out.println(1);
          try
         {
             Thread.sleep(100);
         }
         catch(InterruptedException e)
         {
             e.printStackTrace();
         }
         
        }
        
        
    }
  
}

class A
{
    
}

